{
  "filesystem": {
    "command": "npx",
    "args": ["-y", "@modelcontextprotocol/server-filesystem", "{filesystem_path}"],
    "transport": "stdio",
    "enabled": true
  },
  "duckduckgo": {
    "command": "uvx",
    "args": ["duckduckgo-mcp-server"],
    "transport": "stdio",
    "enabled": true
  },
  "fetch": {
    "command": "uvx",
    "args": ["mcp-server-fetch"],
    "transport": "stdio",
    "enabled": false
  },
  "blender": {
    "command": "uvx",
    "args": ["blender-mcp"],
    "transport": "stdio",
    "enabled": false
  },
  "excel": {
    "command": "uvx",
    "args": ["excel-mcp-server", "stdio"],
    "transport": "stdio",
    "enabled": false
  },
  "sequential-thinking": {
    "command": "npx",
    "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
    "transport": "stdio",
    "enabled": true
  },
  "win-cli": {
    "command": "npx",
    "args": ["-y", "@simonb97/server-win-cli", "--config", "./win-cli-config.json"],
    "transport": "stdio",
    "enabled": true
  }
}
